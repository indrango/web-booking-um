<body>

<div class="wrapper">
    <div class="sidebar" data-color="purple">
      <div class="sidebar-wrapper">
            <div class="logo">
                <h3 class="simple-text">
                    Admin Panel
                </h3>
            </div>
            <ul class="nav">
                <li class="active">
                    <form action="/admin/dashboard/" method="get">
                        <button class="btn pe-7s-users"> DATA MAHASISWA</button>
                    </form>
                </li>
                <li>
                    <form action="/admin/dashboard/kursus" method="get">
                        <button class="btn pe-7s-users"> DATA KURSUS</button>
                    </form>
                </li>
                <li>
                    <form action="/admin/logout" method="post">
                        <button class="btn pe-7s-users"> LOGOUT</button>
                    </form>
                </li>
            </ul>
      </div>
    </div>

    <div class="main-panel">
        <nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" href="#">Data Kursus</a>
                </div>
            </div>
        </nav>

    <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <!-- <h3 class=r"title">Ruang A</h3> -->
                                <p class="category"> <%= today %> </p>
                            </div>
                            <div class="content table-responsive table-full-width">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col">ID</th>
                                            <th scope="col">Kode Voucher</th>
                                            <th scope="col">NPM</th>
                                            <th scope="col">Nama</th>
                                            <th scope="col">Jenis Kelamin</th>
                                            <th scope="col">Tanggal Lahir</th>
                                            <th scope="col">Fakultas</th>
                                            <th scope="col">Jurusan</th>
                                            <th scope="col">Email</th>
                                            <th scope="col">Urutan</th>
                                            <th scope="col">Sesi UM</th>
                                            <th scope="col">Ruang UM</th>
                                            <th scope="col">Meja UM</th>
                                            <th scope="col">Waktu Daftar</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% users.forEach(function(user) { %>  
                                        <tr>
                                            <td><%= user.id_booking %></td>
                                            <td><%= user.kd_voucher %></td>              
                                            <td><%= user.npm_mhs %></td>
                                            <td><%= user.nama_mhs %></td>
                                            <td><%= user.jenis_kel %></td>
                                            <td><%= user.tgl_lahir %></td>
                                            <td><%= user.fakultas %></td>
                                            <td><%= user.jurusan %></td>
                                            <td><%= user.email %></td>
                                            <td><%= user.urutan %></td> 
                                            <td><%= user.sesi_um %></td>
                                            <td><%= user.ruang_um %></td>
                                            <td><%= user.meja_um %></td>
                                            <td><%= user.waktu %></td>
                                        </tr>
                                        <% }) %>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>

